
generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-arm64-openssl-3.0.x"]
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Lead {
  id            String   @id @default(cuid())
  nome          String
  email         String
  telefone      String
  data_submissao DateTime @default(now())
  
  @@map("leads")
}
